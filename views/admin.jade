doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Admin
    body 
        a(href="/") 
            input#home(type="button", value="Home")        
        input#search(type="search", name="Find")
        br
        table
            thead 
                tr: th ID
                    th Name 
                    th Sentiment 
            tbody 
                each item, index in devices
                    tr: td=item.DeviceID
                        td=item.DeviceName
                        td=sentiment[index].AvgSentiment
        div=msg